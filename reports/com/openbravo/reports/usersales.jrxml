	<queryString>
		<![CDATA[SELECT PEOPLE.NAME, MIN(RECEIPTS.DATENEW) AS STARTDATE, MAX(RECEIPTS.DATENEW) AS ENDDATE, SUM(TICKETLINES.UNITS * TICKETLINES.PRICE * (1+TAXES.RATE)) AS TOTAL, CLOSEDCASH.HOSTSEQUENCE, MIN(CLOSEDCASH.DATESTART) AS CASHDATESTART, MAX(CLOSEDCASH.DATEEND) AS CASHDATEEND
FROM PEOPLE, (select * from RECEIPTS UNION ALL select * from BRANCH_RECEIPTS) RECEIPTS, (select * from TICKETS UNION ALL select * from BRANCH_TICKETS) TICKETS, (select * from TICKETLINES UNION ALL select * from BRANCH_TICKETLINES) TICKETLINES, CLOSEDCASH, TAXES 
WHERE PEOPLE.ID = TICKETS.PERSON 
    AND RECEIPTS.ID = TICKETS.ID 
    AND TICKETS.ID = TICKETLINES.TICKET 
    AND RECEIPTS.MONEY=CLOSEDCASH.MONEY
    AND TAXES.ID=TICKETLINES.TAXID
GROUP BY PEOPLE.NAME, CLOSEDCASH.HOSTSEQUENCE
ORDER BY PEOPLE.NAME, CLOSEDCASH.HOSTSEQUENCE]]>
	</queryString>
