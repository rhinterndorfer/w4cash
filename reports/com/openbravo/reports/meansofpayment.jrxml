	<queryString>
		<![CDATA[select 
	cc.HOSTSEQUENCE as CASHSEQUENCE, 
	cc.DATESTART, 
	cc.DATEEND,
	case p.PAYMENT when 'cash' then 'Bar' when 'cashin' then 'Einzahlung' when 'cashout' then 'Auszahlung' when 'magcard' then 'Karte' when 'free' then 'Frei' when 'paperin' then 'Rechnung' else p.PAYMENT end as PAYMENT,
	p.TOTAL
from 
	CLOSEDCASH cc
	inner join 
		RECEIPTS r
	on cc.MONEY=r.MONEY 
	inner join
		PAYMENTS p
	on r.ID=p.RECEIPT
order by p.PAYMENT]]>
	</queryString>
